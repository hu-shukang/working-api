{
  "name": "working-api",
  "version": "1.0.0",
  "description": "Serverless aws-nodejs-typescript template",
  "main": "serverless.ts",
  "scripts": {
    "lint:js": "eslint --ext \".ts\" --ignore-path .gitignore .",
    "lint:prettier": "prettier --check .",
    "lint": "yarn lint:js && yarn lint:prettier",
    "lint:fix": "prettier --write --list-different . && yarn lint:js --fix",
    "sls:remove:it": "serverless remove --stage it",
    "sls:remove:st": "serverless remove --stage st",
    "sls:deploy:it": "serverless deploy --stage it",
    "sls:deploy:st": "serverless deploy --stage st",
    "sls:info:it": "serverless info --stage it",
    "sls:invoke:getToken": "serverless invoke local --stage it --function getToken --path src/functions/user/token/get/mock.json"
  },
  "engines": {
    "node": ">=14.15.0"
  },
  "dependencies": {},
  "devDependencies": {
    "@aws-sdk/client-dynamodb": "^3.409.0",
    "@aws-sdk/client-ssm": "^3.409.0",
    "@aws-sdk/lib-dynamodb": "^3.409.0",
    "@middy/core": "^3.4.0",
    "@middy/http-cors": "^4.6.1",
    "@middy/http-error-handler": "^4.6.1",
    "@middy/http-json-body-parser": "^3.4.0",
    "@middy/validator": "^4.6.1",
    "google-auth-library": "^9.0.0",
    "@serverless/typescript": "^3.0.0",
    "@types/aws-lambda": "^8.10.71",
    "@types/node": "^14.14.25",
    "@typescript-eslint/eslint-plugin": "^6.5.0",
    "@typescript-eslint/parser": "^6.5.0",
    "esbuild": "^0.14.11",
    "eslint": "^8.48.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "json-schema-to-ts": "^1.5.0",
    "prettier": "3.0.3",
    "serverless": "^3.0.0",
    "serverless-dotenv-plugin": "^6.0.0",
    "serverless-esbuild": "^1.23.3",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.1.3"
  },
  "author": "The serverless webpack authors (https://github.com/elastic-coders/serverless-webpack)",
  "license": "MIT"
}
